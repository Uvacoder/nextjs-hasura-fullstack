    "@retail-ui/core": "^0.6.11",
    "@tailwindcss/ui": "^0.6.0",
    "@types/react-beautiful-dnd": "^13.0.0",
    "clsx": "^1.1.1",
    "dayjs": "^1.8.35",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "graphql": "^15.3.0",
    "immer": "^7.0.9",
    "isomorphic-unfetch": "^3.0.0",
    "isomorphic-ws": "^4.0.1",
    "jsonwebtoken": "^8.5.1",
    "next": "^9.5.3",
    "next-auth": "^3.1.0",
    "pg": "^8.3.3",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "react": "16.13.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "16.13.1",
    "react-scrollbars-custom": "^4.0.25",
    "react-use": "^15.3.4",
    "subscriptions-transport-ws": "^0.9.18",
    "tailwindcss": "^1.8.8",
    "zustand": "^3.1.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.17.8",
