  "scripts": {
    "dev": "concurrently \"next dev\" \"yarn generate --watch\"",
    "build:analyze": "ANALYZE=true next build && tsc --project tsconfig.server.json",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "lint": "eslint --fix .",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "generate": "graphql-codegen --config codegen.yml --require dotenv/config",
    "postinstall": "npx @prisma/cli generate"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
